{
  "name": "photo-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "webpack --mode production",
    "clean": "rm -r dist",
    "clean-packages": "rm -rf node_modules",
    "lint": "tsc && eslint ./src --ext=js,ts,tsx --fix",
    "rebuild": "npm run clean && npm run build",
    "reinstall": "npm run clean-packages && rm package-lock.json && npm install",
    "start": "webpack serve --mode development --env development"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.7",
    "@types/node": "^12.19.4",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-netlify-form": "^2.1.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/webpack-env": "^1.15.3",
    "babel-loader": "^8.2.1",
    "copy-webpack-plugin": "^6.4.0",
    "html-webpack-plugin": "^4.5.1",
    "html-webpack-tags-plugin": "^2.0.17",
    "mini-css-extract-plugin": "^1.3.0",
    "node-sass": "^4.14.1",
    "preload-webpack-plugin": "^3.0.0-beta.3",
    "raw-loader": "^4.0.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-netlify-form": "^2.1.5",
    "react-photo-gallery": "^8.0.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "robotstxt-webpack-plugin": "^7.0.0",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "terser-webpack-plugin": "^4.2.3",
    "ts-loader": "^8.0.11",
    "typescript": "^3.9.7",
    "web-vitals": "^1.1.0",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.0",
    "workbox-webpack-plugin": "^6.0.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "prettier": "^2.2.1"
  }
}
